{
  "name": "defi-yield-aggregator",
  "version": "1.0.0",
  "description": "Comprehensive DeFi yield aggregator that monitors all protocols and automates yield optimization",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "scraper": "node src/scrapers/index.js",
    "analyzer": "node src/analyzers/index.js",
    "dashboard": "node src/dashboard/server.js",
    "test": "jest",
    "build": "webpack --mode production",
    "setup": "npm install && node scripts/setup.js",
    "euler:test": "node src/euler-test.js",
    "euler:docker": "docker-compose -f docker-compose.euler.yml up",
    "euler:docker:build": "docker-compose -f docker-compose.euler.yml build",
    "euler:docker:down": "docker-compose -f docker-compose.euler.yml down",
    "web": "node src/web/server.js",
    "universal:test": "node src/universal-test.js",
    "full:demo": "node src/full-system-demo.js"
  },
  "keywords": [
    "defi",
    "yield-farming",
    "aggregator",
    "automation",
    "ethereum",
    "polygon",
    "arbitrum",
    "optimism"
  ],
  "author": "DeFi Yield Team",
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.8.1",
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.6.1",
    "node-cron": "^3.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "ws": "^8.14.2",
    "sqlite3": "^5.1.6",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^2.4.2",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "bn.js": "^5.2.1",
    "web3": "^4.3.0",
    "@uniswap/sdk-core": "^4.2.1",
    "@uniswap/v3-sdk": "^3.11.2",
    "coingecko-api": "^1.0.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/defi-yield-aggregator.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
